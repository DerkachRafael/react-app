    "es6",
    "es5",
    "babel"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/DerkachRafael/package-test.git"
  },
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "del-cli": "^1.1.0",
    "markdown-it": "^8.4.0",
    "mocha": "^5.0.5",
    "prettier": "^1.9.2",
    "chai": "^4.1.2",
    "assert": "^1.4.1"
  },
  "engines": {
    "node": ">=8.11.1",
