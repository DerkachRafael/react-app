{
  "name": "jsmp-infra-useful-snippets-js",
  "version": "1.1.6",
  "description": "useful js funcction on every day",
  "main": "./lib/index.js",
  "scripts": {
    "build": "npm run clean && npm run compile && npm run test",
    "compile": "babel src --out-dir lib --presets=es2015,stage-0",
    "watch": "babel src --watch --out-dir lib",
    "clean": "rimraf lib/",
    "test": "mocha --compilers js:babel-core/register test/**/*.js"
  },
